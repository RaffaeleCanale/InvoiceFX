<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.StackPane?>
<StackPane alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/8"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.gui.google.StatusPaneController">
    <!--<ImageView fx:id="imageView" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true"/>-->
    <Label fx:id="label" graphicTextGap="8"/>
    <FlowPane mouseTransparent="true">
        <ProgressIndicator fx:id="progress" prefHeight="32.0" prefWidth="32.0"/>
    </FlowPane>
</StackPane>
